<view><view class="background"></view><view class="coachMsg"><block wx:if="{{myCoachMsg.coachPhoto}}"><image class="pic" src="{{cdn+'/coach/'+myCoachMsg.coachPhoto}}" mode="aspectFill"></image></block><block wx:else><view class="pic" style="text-align:center;line-height:100rpx;font-size:18rpx;">暂无头像</view></block><view class="info"><view class="name">{{myCoachMsg.coachName}}</view><view class="school">{{myCoachMsg.school}}</view></view><view class="score"><uni-rate vue-id="4237206c-1" value="{{myCoachMsg.evaluation}}" readonly="{{true}}" size="40rpx" bind:__l="__l"></uni-rate><text style="font-size:20rpx;margin-left:14rpx;color:#666666;">{{myCoachMsg.evaluation}}</text></view><view class="labels"><block wx:for="{{myCoachMsg.tags}}" wx:for-item="k" wx:for-index="i"><view class="{{['son',labelStyle[i]]}}">{{''+k+''}}</view></block></view></view><view class="coachEvaluation"><view class="title">教练评价</view><view class="score"><uni-rate bind:input="__e" vue-id="4237206c-2" size="{{46}}" allowHalf="true" readonly="{{coachEvaluation.scoreReadonly}}" value="{{coachEvaluation.score}}" data-event-opts="{{[['^input',[['__set_model',['$0','score','$event',[]],['coachEvaluation']]]]]}}" bind:__l="__l"></uni-rate><text style="color:#666666;font-size:22rpx;margin-left:30rpx;">{{$root.g0}}</text></view><view class="form"><u-subsection vue-id="4237206c-3" fontSize="26" list="{{['科目二','科目三']}}" current="{{currNav}}" data-event-opts="{{[['^change',[['sectionChange']]]]}}" bind:change="__e" bind:__l="__l"></u-subsection><view hidden="{{!(currNav==0)}}"><checkbox-group class="content-lable"><block wx:for="{{TypeList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><label data-event-opts="{{[['tap',[['section_label',[i]]]]]}}" style="min-width:50%;margin:10rpx 0;" bindtap="__e"><view><checkbox value="false" checked="{{item.bool}}"></checkbox>{{item.name+''}}</view></label></block></checkbox-group></view><view class="input"><text class="label">{{'本次练车问题反馈('+$root.g1+'/200)'}}</text><text
button::after {
  border: none;
}
page {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #f2f3f5;
}
.background {
  position: fixed;
  top: 0;
  left: 0;
  z-index: -2;
  width: 750rpx;
  height: 300rpx;
}
.userMsg,
.function,
.manage {
  box-sizing: border-box;
}
.userMsg {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  height: 170rpx;
  padding: 0 30rpx;
  margin-top: 40rpx;
  background: #ffffff;
  border-radius: 16rpx;
}
.userMsg .msg {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.userMsg .msg image {
  width: 100rpx;
  height: 100rpx;
  background: #f2f3f5;
  border-radius: 50%;
}
.userMsg .msg .text {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin-left: 30rpx;
  text-align: left;
  color: #111111;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
}
.userMsg .msg .text text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 300rpx;
}
.userMsg .msg .text .name {
  margin-bottom: 16rpx;
  font-size: 40rpx;
  font-weight: 700;
}
.userMsg .msg .text .phone {
  opacity: 0.8;
  font-size: 28rpx;
  font-weight: 400;
}
.userMsg .signIn {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 130rpx;
  height: 55rpx;
  background: #ffffff;
  border: 1rpx solid #ffbb1e;
  box-sizing: border-box;
  border-radius: 29rpx;
}
.userMsg .signIn image {
  margin-right: 7rpx;
  width: 30rpx;
  height: 30rpx;
}
.userMsg .signIn text {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #ffbb1e;
}
.function {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  margin-top: 20rpx;
  width: 690rpx;
  height: 344rpx;
  background: #ffffff;
  border-radius: 16rpx;
}
.function view {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 100%;
}
.function .up view {
  position: relative;
  width: 310rpx;
  height: 140rpx;
}
.function .up view image {
  position: absolute;
  top: 0;
  left: 0;
  width: 310rpx;
  height: 140rpx;
}
.function .up view .name {
  position: absolute;
  top: 23rpx;
  left: 31rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #ffffff;
}
.function .up view .num {
  position: absolute;
  top: 73rpx;
  left: 31rpx;
  font-size: 34rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: left;
  color: #ffffff;
}
.function .down .son {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.function .down .son image {
  margin-bottom: 16rpx;
  width: 70rpx;
  height: 70rpx;
}
.function .down .son text {
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.manage {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20rpx;
  width: 690rpx;
  min-height: 470rpx;
  background: #ffffff;
  border-radius: 16rpx;
}
.manage .title {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 24rpx;
}
.manage .title .text {
  font-size: 34rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: left;
  color: #333333;
}
.manage .title .addCar {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.manage .title .addCar .addIcon {
  margin-left: 8rpx;
  width: 30rpx;
  height: 30rpx;
  line-height: 25rpx;
  text-align: center;
  border-radius: 50%;
  color: #fff;
  font-size: 30rpx;
  background-color: #29c563;
}
.manage .title .addCar text {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.manage .noCar {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 40rpx;
}
.manage .noCar image {
  margin-bottom: 30rpx;
  width: 326rpx;
  height: 196rpx;
}
.manage .noCar text {
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #999999;
}
.manage .carLists {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.manage .carLists .son {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 20rpx;
  margin-bottom: 30rpx;
  box-sizing: border-box;
  width: 642rpx;
  background: #f2f3f5;
  border-radius: 8rpx;
}
.manage .carLists .son .up {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 28rpx 0;
  border-bottom: 2rpx solid #e3e3e3;
  width: 100%;
}
.manage .carLists .son .up .name {
  font-size: 34rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
}
.manage .carLists .son .up .plate {
  display: flex;
}
.manage .carLists .son .up .plate .province {
  width: 34rpx;
  height: 34rpx;
  background: #333333;
  border: 1rpx solid #333333;
  border-radius: 2rpx;
  line-height: 34rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #f2f3f5;
}
.manage .carLists .son .up .plate .num {
  padding: 0 4rpx;
  height: 34rpx;
  line-height: 34rpx;
  background: #f2f3f5;
  border: 1rpx solid #333333;
  border-radius: 2rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.manage .carLists .son .down {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  width: 100%;
}
.manage .carLists .son .down .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.manage .carLists .son .down .item .text {
  margin-top: 16rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #666666;
}
.manage .carLists .son .down .item .data .num {
  font-size: 30rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #f93030;
}
.manage .carLists .son .down .item .data .uiti {
  font-size: 20rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #f55549;
}


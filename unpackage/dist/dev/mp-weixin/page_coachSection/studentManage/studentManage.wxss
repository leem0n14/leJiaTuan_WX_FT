page {
  background-color: #f2f3f5;
}
.hd {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
  position: relative;
  width: 750rpx;
  height: 210rpx;
  padding: 45rpx 30rpx;
  box-sizing: border-box;
}
.hd .bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 210rpx;
  z-index: -2;
}
.hd text {
  color: #fff;
  font-size: 30rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
}
.hd .studentNum {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.hd .historyStudent {
  display: flex;
  align-items: center;
  justify-content: center;
}
.hd .historyStudent image {
  margin-right: 5rpx;
  width: 30rpx;
  height: 30rpx;
}
.bd {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;
}
.bd .searchBar {
  height: 120rpx;
  width: 100%;
  background-color: white;
}
.bd .nav {
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  width: 100%;
  height: 120rpx;
}
.bd .nav .son {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 0 30rpx;
}
.bd .nav .son .redCircle {
  position: absolute;
  top: 0rpx;
  right: -8rpx;
  width: 12rpx;
  height: 12rpx;
  background: #f93030;
  border-radius: 50%;
}
.bd .nav .son .bottom {
  position: absolute;
  bottom: -16rpx;
  width: 62rpx;
  height: 6rpx;
  background: #29c563;
}
.bd .ul {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bd .ul .list {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  max-height: 450rpx;
  background: #ffffff;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  padding: 30rpx;
}
.bd .ul .list .box1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
}
.bd .ul .list .box1 .left {
  display: flex;
  flex-direction: row;
}
.bd .ul .list .box1 .left image {
  width: 96rpx;
  height: 96rpx;
  background: skyblue;
  border-radius: 50%;
}
.bd .ul .list .box1 .left .text {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.bd .ul .list .box1 .left .text .name {
  font-size: 34rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: left;
  color: #333333;
}
.bd .ul .list .box1 .left .text .phone {
  margin-top: 25rpx;
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
}
.bd .ul .list .box1 .right {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.bd .ul .list .box1 .right .change {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 28rpx;
}
.bd .ul .list .box1 .right .change image {
  margin-right: 10rpx;
  width: 33rpx;
  height: 28rpx;
}
.bd .ul .list .box1 .right .change text {
  line-height: 28rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
}
.bd .ul .list .box1 .right .progress {
  display: flex;
  align-items: center;
}
.bd .ul .list .box1 .right .progress text {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
}
.bd .ul .list .box2 {
  display: flex;
  flex-direction: column;
  width: 690rpx;
  height: 250rpx;
  margin: 30rpx 0;
  background: #ffffff;
  border: 2rpx solid #f2f3f5;
  border-radius: 16rpx;
}
.bd .ul .list .box2 .title {
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 30rpx;
  width: 100%;
  font-size: 30rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
}
.bd .ul .list .box2 .title .getDetail {
  color: #2dc666;
}
.bd .ul .list .box2 .progressBars {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 150rpx;
}
.bd .ul .list .box2 .progressBars .son {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bd .ul .list .box2 .progressBars .son .bar {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.bd .ul .list .box2 .progressBars .son .bar .circle {
  position: relative;
  top: 0rpx;
  left: 11rpx;
  width: 84rpx;
  height: 84rpx;
  box-sizing: border-box;
  border: 6rpx solid #b5ebc5;
  border-radius: 50%;
}
.bd .ul .list .box2 .progressBars .son .bar text {
  position: absolute;
  top: 23rpx;
  left: 0;
  width: 100%;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 400;
  text-align: center;
  color: #29c563;
}
.bd .ul .list .box2 .progressBars .son .finished {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
  background-color: #fff;
}
.bd .ul .list .box2 .progressBars .son .finished .border {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 84rpx;
  height: 84rpx;
  box-sizing: border-box;
  border: 6rpx solid #b5ebc5;
  border-radius: 50%;
}
.bd .ul .list .box2 .progressBars .son .text {
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.bd .ul .list .box2 .progressBars .son .line {
  position: absolute;
  top: 49rpx;
  right: -68rpx;
  width: 60rpx;
  height: 4rpx;
  background: #ededed;
  border-radius: 2rpx;
}
.bd .ul .list .box3 {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  margin-top: 10rpx;
}
.bd .ul .list .box3 button {
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 34rpx;
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
}
.bd .ul .list .box3 .accept {
  width: 390rpx;
  background: #ffffff;
  border: 2rpx solid #29c563;
  color: #29c563;
}
.bd .ul .list .box3 .reject {
  width: 280rpx;
  background: #ffffff;
  border: 2rpx solid #e3e3e3;
  color: #666666;
}
.bd .noStudents {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 247rpx;
}
.bd .noStudents image {
  margin-bottom: 30rpx;
  width: 326rpx;
  height: 167rpx;
}
.bd .noStudents text {
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #999999;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 99;
}


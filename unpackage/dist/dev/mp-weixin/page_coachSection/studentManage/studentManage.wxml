<view><view class="hd"><image class="bg" src="{{cdn+'/coachSection/background/studentNum.png'}}" mode="aspectFill"></image><view class="studentNum"><text class="text" style="opacity:0.6;">学员数量</text><text class="num" style="font-size:60rpx;margin-top:20rpx;font-weight:700;">{{stuList.totalCount?stuList.totalCount:''}}</text></view><view class="historyStudent"><image src="{{cdn+'/icon/countdown.svg'}}" mode="aspectFill"></image><text>历史学员</text></view></view><view class="bd"><view class="searchBar"><uni-search-bar vue-id="03214b72-1" placeholder="搜索学员姓名" value="{{studentName}}" data-event-opts="{{[['^confirm',[['searchStudent']]],['^input',[['__set_model',['','studentName','$event',[]]]]]]}}" bind:confirm="__e" bind:input="__e" bind:__l="__l"></uni-search-bar></view><view class="nav"><block wx:for="{{navList}}" wx:for-item="k" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['navAct',['$0'],[[['navList','',i]]]]]]]}}" class="son" bindtap="__e"><text style="{{'color:'+(k.isAct?'#111111':'#999999')+';'+('font-size:'+(k.isAct?'34rpx':'30rpx')+';')+('font-weight:'+(k.isAct?'700':'400')+';')}}">{{k.text+''}}</text><view hidden="{{!(i!=0&&haveNew)}}" class="redCircle"></view><view hidden="{{!(k.isAct)}}" class="bottom"></view></view></block></view><block wx:if="{{$root.g0}}"><view class="ul"><block wx:for="{{$root.l1}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list"><view class="box1"><view class="left"><image class="userPic" src="{{cdn+'/student/'+item.$orig.photo}}" mode="aspectFill"></image><view class="text"><text class="name">{{item.$orig.stuName}}</text><text class="phone">{{item.$orig.stuPhone}}</text></view></view><view class="right"><view class="progress"><text class="course" style="color:#333333;">{{"考试进度："+(item.m0?item.m1:'无进度')}}</text><block wx:if="{{item.m2}}"><text class="state" style="color:#FFBB1E;">{{item.m3}}</text></block></view></view></view><view class="box2"><view class="title"><view>学车进度</view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item:item.$orig})}}" class="getDetail" bindtap="__e">查看</view></view><view class="progressBars"><block wx:for="{{item.l0}}" wx:for-item="k" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" data-event-params="{{({i,index})}}" class="son" bindtap="__e"><block wx:if="{{(i+1)*10>=stuList.list[index].progress}}"><view class="bar"><view class="circle"><text class="num">{{stuList.list[index].progress/10===i+1?'学习中':'未开启'}}</text></view></view></block><block wx:else><view class="finished"><view class="border"><icon class="icon" type="success" size="62rpx" color="#38cb6f"></icon></view></view></block><text>{{k.m4}}</text><view hidden="{{!(i!=3)}}" class="line" style="{{'background:'+(stuList.list[index].progress/10>i+1?'linear-gradient(134deg,#54d685, #29c563 99%)':'')+';'}}"></view></view></block></view></view><block wx:if="{{false}}"><view class="box3"><button data-event-opts="{{[['tap',[['alert']]]]}}" class="reject" bindtap="__e">拒绝接收</button><button data-event-opts="{{[['tap',[['alert']]]]}}" class="accept" bindtap="__e">接收学员</button></view></block></view></block><uni-pagination vue-id="03214b72-2" current="{{pageIndex}}" total="{{stuList.totalCount}}" title="标题文字" show-icon="{{true}}" data-event-opts="{{[['^change',[['changePage']]]]}}" bind:change="__e" bind:__l="__l"></uni-pagination></view></block><block wx:else><view class="noStudents"><image src="{{cdn+'/image/noList/noStudent.png'}}" mode="aspectFill"></image><text>暂无学员</text></view></block></view></view>
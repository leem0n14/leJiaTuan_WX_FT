<view><block wx:if="{{code!=0}}"><view class="box"><view data-event-opts="{{[['tap',[['takePhoto']]]]}}" class="takePhoto" bindtap="__e"><view><image style="height:100%;width:100%;" src="{{imageSrc}}" mode="aspectFill"></image></view><text>拍照</text></view><view data-event-opts="{{[['tap',[['getAuth']]]]}}" class="position" bindtap="__e"><text style="margin-right:35rpx;color:#666666;">定位</text><text style="color:#333333;width:380rpx;">{{position}}</text><image src="{{cdn+'/icon/locationGreen.svg'}}" mode="aspectFill"></image></view><view class="venue"><text>打卡场地</text><picker range-key="siteName" range="{{venueList}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{venue?venue:'请输入打卡场地'}}</view></picker></view></view></block><block wx:if="{{code!=0}}"><button class="sub" disabled="{{imageSrc==null||position=='点击获取地址信息'||venue==null}}" data-event-opts="{{[['tap',[['upLoadPhoto']]]]}}" bindtap="__e">考勤打卡</button></block><block wx:if="{{code!=0}}"><view class="nowTime">{{checkTime}}</view></block><block wx:if="{{code==0}}"><view class="bd"><icon style="margin-top:140rpx;" type="success" color="#29c563" size="155rpx"></icon><text class="font1" style="margin-top:30rpx;">打卡成功</text><view class="address" style="margin-top:40rpx;"><image src="{{cdn+'/icon/location.svg'}}" mode="aspectFill"></image><text class="font2">{{address==null?'':address}}</text></view><text class="font2" style="margin-top:16rpx;">{{time==null?'':time}}</text></view></block><block wx:if="{{code==0}}"><button data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="backIndex" bindtap="__e">返回首页</button></block></view>
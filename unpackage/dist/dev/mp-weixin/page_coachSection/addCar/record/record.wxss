/* 按钮样式 */
button::after {
  border: none;
}
page {
  background-color: #f2f3f5;
}
.empty {
  text-align: center;
  margin-top: 10rpx;
  color: #999999;
}
.d {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}
.d .w {
  width: 750rpx;
  height: 40rpx;
  background-color: #fff;
}
.d .top {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  width: 750rpx;
  height: 88rpx;
  background-color: #fff;
}
.d .top .back {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 88rpx;
  height: 88rpx;
}
.d .top .back image {
  width: 18rpx;
  height: 33rpx;
}
.d .top text {
  line-height: 88rpx;
  height: 88rpx;
  font-size: 34rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
}
.ul {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.ul .li {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;
  width: 690rpx;
  min-height: 164rpx;
  box-sizing: border-box;
  padding: 30rpx;
  background: #ffffff;
  border-radius: 16rpx;
}
.ul .li .title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
  margin-bottom: 42rpx;
}
.ul .li .title .time {
  font-size: 36rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: left;
  color: #333333;
}
.ul .li .title .lineDay {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: right;
  color: #333333;
}
.ul .li .title .lineDay text {
  font-size: 30rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: right;
  color: #f55549;
}
.ul .li .content {
  position: relative;
  display: flex;
  flex-direction: column;
  width: 100%;
}
.ul .li .content .option {
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  margin-bottom: 24rpx;
}
.ul .li .content .option .key {
  min-width: 130rpx;
  color: #999999;
}
.ul .li .content .option .value {
  overflow-wrap: normal;
  color: #333333;
}
.ul .li .content .money {
  position: absolute;
  right: 0;
  display: flex;
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
}
.ul .li .content .money view {
  display: flex;
  color: #333333;
}
.ul .li .content .money view text {
  display: flex;
  color: #f55549;
}


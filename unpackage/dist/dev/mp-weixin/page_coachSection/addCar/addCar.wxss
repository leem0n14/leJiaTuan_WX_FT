.box1 {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  height: 250rpx;
  box-sizing: border-box;
  padding: 40rpx 0 0rpx;
  background: #ffffff;
}
.box1 .input {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 690rpx;
  height: 90rpx;
  margin-bottom: 30rpx;
  background: #f2f2f2;
  border-radius: 8rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.box1 .input .t1 {
  margin-right: 30rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #666666;
}
.box1 .input .t2 {
  display: flex;
  align-items: center;
  width: 450rpx;
  height: 90rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
}
.box1 .input .t2 .font {
  position: relative;
  background: #29c563;
  border-radius: 2rpx;
  text-align: center;
  width: 46rpx;
  height: 46rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  line-height: 46rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  color: #fff;
  margin-right: 10rpx;
}
.box1 .input .t2 .font::after {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 0;
  height: 0;
  content: '';
  border-bottom: 7rpx solid #000;
  border-right: 7rpx solid #000;
  border-top: 7rpx solid #29c563;
  border-left: 7rpx solid #29c563;
}
.box1 .input .t2 picker {
  width: 100%;
  height: 100%;
}
.box1 .input .t2 picker view {
  line-height: 90rpx;
}
.box1 .input image {
  position: absolute;
  right: 30rpx;
  width: 25rpx;
  height: 18rpx;
}
.box2 {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
  width: 750rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.box2 .son {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  min-width: 330rpx;
  min-height: 312rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
  background-color: #ffffff;
  border-radius: 16rpx;
  margin-bottom: 30rpx;
}
.box2 .son .up {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
}
.box2 .son .up .name {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.box2 .son .up .name image {
  margin-right: 10rpx;
  width: 38rpx;
  height: 38rpx;
}
.box2 .son .up .name text {
  font-size: 30rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Bold;
  font-weight: 700;
  text-align: left;
  color: #333333;
}
.box2 .son .up .num {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.box2 .son .up .num text {
  line-height: 24rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: right;
  color: #666666;
}
.box2 .son .up .num image {
  width: 17rpx;
  height: 17rpx;
}
.box2 .son .mid {
  font-size: 26rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.box2 .son .down {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  height: 60rpx;
  background: #e8fbef;
  border-radius: 30rpx;
}
.box2 .son .down image {
  margin-right: 4rpx;
  width: 26rpx;
  height: 26rpx;
}
.box2 .son .down view {
  line-height: 30rpx;
  font-size: 30rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #29c563;
}

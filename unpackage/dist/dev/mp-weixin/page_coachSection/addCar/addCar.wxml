<view><view class="box1"><view class="input"><view class="t1">车型信息</view><view class="t2"><input style="width:450rpx;" type="text" disabled="{{isBan}}" placeholder="请输入车型信息(例:比亚迪白色)" data-event-opts="{{[['input',[['__set_model',['','carType','$event',[]]]]]]}}" value="{{carType}}" bindinput="__e"/></view><image src="{{cdn+'/icon/triangle.svg'}}" mode="aspectFill"></image></view><view class="input"><view class="t1">车牌号码</view><view data-event-opts="{{[['tap',[['input',['$event']]]]]}}" class="t2" bindtap="__e"><input class="font" type="text" placeholder-style="color:#fff" disabled="{{isBan}}" data-event-opts="{{[['input',[['__set_model',['','province','$event',[]]]]]]}}" value="{{province}}" bindinput="__e"/><input style="width:400rpx;" type="text" placeholder="请输入车牌号(左边输入省份)" disabled="{{isBan}}" maxlength="6" data-event-opts="{{[['input',[['__set_model',['','carId','$event',[]]]]]]}}" value="{{carId}}" bindinput="__e"/></view></view></view><view class="box3"><view class="type son"><view class="text">驾照类型</view><picker class="typeChoose" mode="selector" range="{{typeName}}" data-event-opts="{{[['change',[['changeType',['$event']]]]]}}" bindchange="__e"><view>{{type}}</view></picker><image class="triangle" src="{{cdn+'/icon/triangle.svg'}}" mode></image></view></view><view class="box2"><block wx:for="{{msgList}}" wx:for-item="k" wx:for-index="i" wx:key="i"><block wx:if="{{carId?$root.g0:0>=6}}"><view class="son"><view class="up"><view class="name"><image src="{{cdn+'/coachSection/icon/'+k.pic}}" mode="aspectFill"></image><text>{{k.name}}</text></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({k,i})}}" class="num" bindtap="__e"><text>{{"共"+k.num+"条"}}</text><image src="/static/images/icon/arrow-right-bold.png" mode="aspectFill"></image></view></view><view class="mid"></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" data-event-params="{{({k})}}" class="down" bindtap="__e"><image src="/static/images/icon/add.svg" mode="aspectFill"></image><view>记录</view></view></view></block></block></view><button class="sub" disabled="{{carId==null||carType==null||province==''}}" data-event-opts="{{[['tap',[['add_Car']]]]}}" bindtap="__e">确定</button><block wx:if="{{haveCarid}}"><button data-event-opts="{{[['tap',[['subCar']]]]}}" class="sub1" style="margin-bottom:20rpx;" bindtap="__e">删除</button></block><view hidden="{{!(isShowChoose)}}" class="chooseCar"><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="mask" bindtap="__e"></view><view class="content"><view class="title">是否确定删除</view><view class="content-button"><view data-event-opts="{{[['tap',[['jiedan1']]]]}}" class="queren1" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['jiedan']]]]}}" class="queren" bindtap="__e">确认</view></view></view></view></view>
<view><image class="background" src="{{cdn+'/coachSection/background/background.png'}}" mode="aspectFit"></image><view class="userMsg"><view class="msg"><image src="{{cdn+'/user/'+user.head}}" mode="aspectFill"></image><view class="text"><text class="name">{{user.name}}</text><text class="phone">{{user.phone}}</text></view></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="signIn" bindtap="__e"><image src="{{cdn+'/icon/signIn/signIn.svg'}}" mode="aspectFill"></image><text>签到</text></view></view><view class="function"><view class="up"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="studentsMsg" bindtap="__e"><image class="bg" src="{{cdn+'/coachSection/background/studentMsg.png'}}" mode="aspectFill"></image><text class="name">学员管理</text><text class="num">{{msg.studentNum}}</text></view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" class="booking" bindtap="__e"><image class="bg" src="{{cdn+'/coachSection/background/studentBooking.png'}}" mode="aspectFill"></image><text class="name">学员预约</text><text class="num">{{msg.mpNum}}</text></view></view><view class="down"><block wx:for="{{functionList}}" wx:for-item="k" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['e3',['$event']]]]]}}" data-event-params="{{({k})}}" class="son" bindtap="__e"><image src="{{cdn+'/coachSection/image'+k.pic}}" mode="aspectFill"></image><text>{{k.text}}</text></view></block></view></view><view class="manage"><view class="title"><text class="text">车辆管理</text><view data-event-opts="{{[['tap',[['e4',['$event']]]]]}}" class="addCar" bindtap="__e"><text>添加车辆</text><view class="addIcon">+</view></view></view><block wx:if="{{$root.g0}}"><view class="noCar"><image src="{{cdn+'/coachSection/background/noCar.png'}}" mode="aspectFill"></image><text>暂无车辆</text></view></block><block wx:else><view class="carLists"><block wx:for="{{msg.cars}}" wx:for-item="k" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['e5',['$event']]]]]}}" data-event-params="{{({k})}}" class="son" bindtap="__e"><view class="up"><text class="name">{{k.carType}}</text><text class="name">{{k.carForm}}</text><view class="plate"><view class="num">{{k.carId}}</view></view></view></view></block></view></block></view></view>
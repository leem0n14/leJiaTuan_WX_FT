}
.course23 .videos {
  display: flex;
  flex-direction: column;
  width: 750rpx;
  margin-top: 20rpx;
  background: #ffffff;
}
.course23 .videos .title {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 750rpx;
  height: 100rpx;
  padding: 33rpx 24rpx;
  box-sizing: border-box;
  background: #ffffff;
}
.course23 .videos .title image {
  margin-right: 14rpx;
  width: 34rpx;
  height: 34rpx;
}
.course23 .videos .title text {
  font-size: 32rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;

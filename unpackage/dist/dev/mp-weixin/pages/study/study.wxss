}
.course23 .box3 .option image {
  margin-right: 14rpx;
  width: 34rpx;
  height: 34rpx;
}
.course23 .box3 .option text {
  font-size: 32rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;

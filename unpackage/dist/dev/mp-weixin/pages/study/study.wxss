.nav {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 750rpx;
  height: 88rpx;
  margin-bottom: 4rpx;
  background: #ffffff;
}
.nav .list {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 25%;
  height: 100%;
}
.nav .list text {
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #999999;
}
.nav .list view {
  position: absolute;
  bottom: 0;
  width: 98rpx;
  height: 4rpx;
  background: #29c563;
}
.otherNav {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 750rpx;
  height: 100rpx;
  background: #ffffff;
}
.otherNav .son {
  margin: 0 3rpx;
  width: 230rpx;
  height: 60rpx;
  border-radius: 2rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #f55549;
  line-height: 60rpx;
}
.otherNav .st {
  color: #f55549;
  background: #ffe5e4;
}
.otherNav .nd {
  color: #f59211;
  background: #ffedc5;
}
.otherNav .rd {
  background: #b8f2c6;
  color: #4bb23d;
}
.box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 750rpx;
  height: 380rpx;
  background: #ffffff;
}
.box .center {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 248rpx;
  height: 248rpx;
  box-sizing: border-box;
  border-radius: 50%;
}
.box .center .inside {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 214rpx;
  height: 214rpx;
  border-radius: 50%;
}
.box .center .inside text {
  font-size: 36rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Medium;
  font-weight: 500;
  text-align: center;
  color: #ffffff;
}
.box .mockExam {
  border: 6rpx solid rgba(73, 194, 245, 0.2);
}
.box .mockExam .inside {
  background: linear-gradient(180deg, #49ccf5, #4988f5);
}
.box .practice {
  border: 6rpx solid rgba(66, 206, 80, 0.2);
}
.box .practice .inside {
  background: linear-gradient(134deg, #54d685, #29c563 99%);
}
.box .left,
.box .right {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 208rpx;
  height: 320rpx;
}
.box .left .son,
.box .right .son {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 160rpx;
  width: 100%;
}
.box .left .son image,
.box .right .son image {
  width: 56rpx;
  height: 56rpx;
}
.box .left .son text,
.box .right .son text {
  margin-top: 20rpx;
  font-size: 24rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.questionBank {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
  width: 750rpx;
  height: 100rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
  background: #ffffff;
}
.questionBank .left,
.questionBank .right {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.questionBank .left text {
  margin-left: 10rpx;
  font-size: 32rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: center;
  color: #333333;
}
.questionBank .left image {
  width: 34rpx;
  height: 31rpx;
}
.questionBank .right text {
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: right;
  color: #999999;
}
.questionBank .right image {
  margin-top: 4rpx;
  width: 20rpx;
  height: 20rpx;
}
.course23 .box2 {
  display: flex;
  align-items: center;
  justify-content: space-between;

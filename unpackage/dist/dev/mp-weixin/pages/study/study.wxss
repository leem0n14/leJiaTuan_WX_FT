  height: 140rpx;
}
.course23 .videos .son .son-image {
  position: absolute;
  left: 0;
  top: 0;
  width: 250rpx;
  height: 140rpx;
  background-color: #333333;
}
.course23 .videos .son .name {
  position: absolute;
  top: 5rpx;
  left: 270rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
}
.course23 .videos .son .text {
  position: absolute;
  top: 51rpx;
  left: 270rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  width: 380rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.course23 .videos .son .difficulty {
  position: absolute;
  bottom: 35rpx;
  left: 270rpx;
  display: flex;
  align-items: center;
}
.course23 .videos .son .difficulty .t {
  font-size: 20rpx;
  font-family: Source Han Sans CN, Source Han Sans CN-Regular;
  font-weight: 400;
  text-align: left;
  color: #c1c1c1;
}

